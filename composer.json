{
    "name": "lav45/yii2-file-upload-module",
    "description": "This is Yii2 extension for the upload files",
    "keywords": [
        "yii2",
        "extension",
        "behavior",
        "widget",
        "upload"
    ],
    "homepage": "https://github.com/lav45/yii2-file-upload-module",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.0.*",
        "bower-asset/blueimp-file-upload": "10.*",
        "diecoding/yii2-flysystem": "^1.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "lav45\\fileUpload\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
